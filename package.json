{
  "name": "lazysearch-js",
  "version": "0.3.1",
  "description": "JavaScript Library for LazySearch",
  "main": "src/lz.js",
  "scripts": {
    "build": "webpack --mode production --bail",
    "watch": "webpack --mode development -d --watch",
    "clear": "rm -r dist/* && touch dist/.gitkeep",
    "test": "jest",
    "lint": "eslint *"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.html$": "<rootDir>/test/utils/htmlLoader.js"
    }
  },
  "eslintIgnore": [
    "LICENSE",
    "webpack.config.js",
    "*.md",
    "*.gif",
    "*.json",
    "dist/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fillin-inc/lazysearch-js.git"
  },
  "keywords": [
    "LazySearch"
  ],
  "author": "fillin Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fillin-inc/lazysearch-js/issues"
  },
  "homepage": "https://lazysear.ch/",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "html-loader": "^0.5.5",
    "jest": "^22.4.4",
    "nock": "^9.3.2",
    "style-loader": "^0.20.3",
    "webpack": "^4.12.0",
    "webpack-cli": "^2.1.5"
  },
  "engines": {
    "node": ">= 8.9",
    "npm": ">= 5.6"
  },
  "dependencies": {
    "escape-html": "^1.0.3",
    "promise-polyfill": "7.1.0",
    "whatwg-fetch": "^2.0.4"
  }
}
